<% page_title 'Your Dashboard' %>

<div class="tasks">
  <ul>
    <% @tasks.each do |task| %>
      <li>
        <%= link_to task.path do %>
          <strong>[<%= task.type %> <i>#<%= task.sequential_id %></i>] <%= task %></strong>
        <% end %>
        (<%= task.contextable %>)
      </li>
    <% end %>
  </ul>
</div>
